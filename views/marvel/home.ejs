<% for (var i=0; i < data.length; i++) { %>
  <div class="card-container h">
      <div class="front">
        <%= data[i].name %>
      </div>
      <div class="back">
        <div class='character-container-card'>
          Back
        </div>
      </div>
  </div>
<% } %>

<% if (!loggedIn) { %>
<p>To See More Characters Please <a href="/login">Log In</a></p>
<% } %>